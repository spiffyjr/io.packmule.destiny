{
  "name": "Packmule for Destiny",
  "description": "Destiny inventory manager with aggregate views, filtering, loadouts, and more!",
  "version": "1.1.0",
  "manifest_version": 2,
  "permissions": [
    "cookies",
    "tabs",
    "*://*.bungie.net/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "asset/img/icon.png",
    "default_title": "Destiny Packmule"
  }
}
